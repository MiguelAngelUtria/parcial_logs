CREATE DATABASE IF NOT EXISTS logs_db;
USE logs_db;

CREATE TABLE logs (
    id INT AUTO_INCREMENT PRIMARY KEY,
    servicio VARCHAR(50) NOT NULL,
    nivel ENUM('INFO', 'WARNING', 'ERROR') NOT NULL,
    mensaje TEXT NOT NULL,
    fecha TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);
